{
  "name": "AIM for Slack",
  "description": "Time travel back to 2002",
  "author": "Jared Schwalbe",
  "version": "1.0.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["*://app.slack.com/client/*"],
      "js": ["app.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["*://app.slack.com/*"],
      "resources": [
        "audio/startup.mp3",
        "audio/door-open.mp3",
        "audio/door-close.mp3",
        "audio/error.wav"
      ]
    }
  ]
}
