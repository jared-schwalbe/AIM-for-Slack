{
  "name": "AIM for Slack",
  "description": "Time travel back to 2002",
  "author": "Jared Schwalbe",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": ["*://app.slack.com/client/*"],
      "css": ["app.css"],
      "js": ["app.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["*://app.slack.com/*"],
      "resources": [
        "app.css",
        "WinXP/assets/audio/startup.mp3",
        "WinXP/assets/audio/error.wav",
        "WinXP/assets/audio/dooropen.wav",
        "WinXP/assets/audio/doorslam.wav",
        "WinXP/assets/audio/imrcv.wav",
        "WinXP/assets/audio/imsend.wav",
        "WinXP/assets/audio/ring.wav"
      ]
    }
  ]
}
